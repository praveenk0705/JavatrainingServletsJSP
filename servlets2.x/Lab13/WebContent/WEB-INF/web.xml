<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Lab13</display-name>
  <welcome-file-list>
    <welcome-file>demo.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>demos</servlet-name>
  <servlet-class>com.jlcindia.servlets.DemoServlet</servlet-class>
  <init-param>
  <param-name>City</param-name>
  <param-value>Blore</param-value>
  </init-param>
  <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  <servlet-name>demos</servlet-name>
  <url-pattern>/demo.jlc</url-pattern>
  </servlet-mapping>
  <filter>
  <filter-name>demof</filter-name>
  <filter-class>com.jlcindia.filters.DemoFilter</filter-class>
  <init-param>
  <param-name>City</param-name>
  <param-value>Hyd</param-value>
  </init-param>
  </filter>
  <filter-mapping>
  <filter-name>demof</filter-name>
  <url-pattern>/demo.jlc</url-pattern>
  </filter-mapping>
</web-app>