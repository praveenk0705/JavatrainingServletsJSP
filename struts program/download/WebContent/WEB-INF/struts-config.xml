<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

<form-beans>
<form-bean name="tempForm" type="org.apache.struts.action.DynaActionForm">
<form-property name="cat" type="java.lang.String"/>
</form-bean>
<form-bean name="loginForm" type="com.jlcindia.struts1.LoginForm"/>
<form-bean name="tempForm"
            type="org.apache.struts.action.DynaActionForm">
<form-property name="downloadname" type="java.lang.String"/>
</form-bean>     
</form-beans>
<global-forwards>
<forward name="download" path="/index1.jsp"/>

 <forward name="success" path="/downloadhome.jsp"/>

</global-forwards>                
<action-mappings>
<action path="/LoginSubmit" name="loginForm" 
		type="com.jlcindia.struts1.DMAction" 
 		input="/index.jsp" parameter="method" >
</action>
<action path="/Download" name="tempForm" type="com.jlcindia.struts1.DownloadAction" parameter="method"
            input="/downloadhome.jsp"/>
 <action path="/Down"  type="com.jlcindia.struts1.DownloadListAction"    scope="session" >

</action>  
 </action-mappings>
             <message-resources parameter="com.jlcindia.struts1.application"/>
             <plug-in className="org.apache.struts.tiles.TilesPlugin">
             <set-property property="definitions-config" value="/WEB-INF/tiles.xml"/>
             </plug-in>
</struts-config>

