<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
<form-beans>
<form-bean name="lForm" type="com.sk.LoginForm"/>


</form-beans>
<action-mappings>
<action path="/Login" name="lForm" type="com.sk.LoginAction" validate="true" input="/login.jsp">
<forward name="success" path="/success.jsp"/>
<forward name="failed" path="/login.jsp"/>
</action>
</action-mappings>
<message-resources parameter="com.sk.message"/>
</struts-config>

